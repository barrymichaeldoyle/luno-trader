{
  "name": "functions",
  "version": "1.0.0",
  "description": "Functions to run Luno Trading",
  "main": "dist/index.js",
  "bin": {
    "ripple": "./bin/ripple"
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "rimraf dist && tsc && pkg ./dist/src/index.js --target latest-macos-x64"
  },
  "pkg": {
    "assets": [
      "node_modules/**/*",
      "src/**/*"
    ],
    "targets": [
      "node12-macos-x64",
      "node10-win-x64"
    ]
  },
  "author": "Barry Michael Doyle",
  "license": "ISC",
  "devDependencies": {
    "@types/base-64": "^0.1.3",
    "@types/node": "^14.14.22",
    "@types/node-fetch": "^2.5.8",
    "@types/prompt-sync": "^4.1.0",
    "nodemon": "^2.0.7",
    "pkg": "^4.4.9",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "base-64": "^1.0.0",
    "cli-select": "^1.1.2",
    "dotenv": "^8.2.0",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.1",
    "path": "^0.12.7",
    "prompt-sync": "^4.2.0"
  },
  "preferGlobal": true
}
